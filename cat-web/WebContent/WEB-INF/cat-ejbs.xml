<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:tx="http://www.springframework.org/schema/tx"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="ejb.UserManager" class="org.springframework.jndi.JndiObjectFactoryBean" scope="request">
		<property name="jndiName" value="ejb/cat/UserManager"/>
	</bean>

	<bean id="ejb.CampaignManager" class="org.springframework.jndi.JndiObjectFactoryBean" scope="request">
		<property name="jndiName" value="ejb/cat/CampaignManager"/>
	</bean>

	<bean id="ejb.ClientManager" class="org.springframework.jndi.JndiObjectFactoryBean" scope="request">
		<property name="jndiName" value="ejb/cat/ClientManager"/>
	</bean>

	<bean id="non.request.ejb.ClientManager" class="org.springframework.jndi.JndiObjectFactoryBean">
		<property name="jndiName" value="ejb/cat/ClientManager"/>
	</bean>

	<bean id="ejb.CouponManager" class="org.springframework.jndi.JndiObjectFactoryBean" scope="request">
		<property name="jndiName" value="ejb/cat/CouponManager"/>
	</bean>

	<bean id="ejb.LayoutManager" class="org.springframework.jndi.JndiObjectFactoryBean" scope="request">
		<property name="jndiName" value="ejb/cat/LayoutManager"/>
	</bean>

	<bean id="ejb.SubscriptionManager" class="org.springframework.jndi.JndiObjectFactoryBean" scope="request">
		<property name="jndiName" value="ejb/cat/SubscriptionManager"/>
	</bean>

	<bean id="ejb.ContactManager" class="org.springframework.jndi.JndiObjectFactoryBean" scope="request">
		<property name="jndiName" value="ejb/cat/ContactManager"/>
	</bean>
</beans>