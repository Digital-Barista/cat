<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
	<persistence-unit name="cat-data">
		<jta-data-source>java:/jdbc/campaignAdminDS</jta-data-source>
		<class>com.digitalbarista.cat.data.CouponCounterDO</class>
		<class>com.digitalbarista.cat.data.CouponOfferDO</class>
		<class>com.digitalbarista.cat.data.CouponRedemptionDO</class>
		<class>com.digitalbarista.cat.data.CouponResponseDO</class>
		<properties>
			<property name="hibernate.dialect" value="org.hibernate.dialect.MySQLDialect"/>
		</properties>
	</persistence-unit>
</persistence>
