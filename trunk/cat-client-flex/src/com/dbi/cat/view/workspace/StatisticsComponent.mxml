<?xml version="1.0" encoding="utf-8"?>
<mx:Canvas xmlns:mx="http://www.adobe.com/2006/mxml" height="25">
	<mx:Script>
		<![CDATA[
			[Bindable]
			[Embed("/assets/swf/blue_circle.swf")]
			private var blueIcon:Class;
		]]>
	</mx:Script>
	<mx:SWFLoader
		width="100%" height="100%"
		minWidth="25" minHeight="25"
		source="{blueIcon}"
		maintainAspectRatio="false"/>
	<mx:Label text="{data}"
		width="100%"
		color="0xFFFFFF"
		fontWeight="bold"
		verticalCenter="0"
		textAlign="center" />
</mx:Canvas>
